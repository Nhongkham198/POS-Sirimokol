rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      // Allow read/write access for the next 30 days for development.
      // After this period, you will need to set up proper security rules.
      allow read, write: if request.time < timestamp.date(2024, 8, 27);
    }
  }
}